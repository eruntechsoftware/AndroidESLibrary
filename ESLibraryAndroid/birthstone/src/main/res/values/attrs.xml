<?xml version="1.0" encoding="utf-8"?>
<resources>
	<attr name="dataType">
		<enum name="String" value="0" />
        <enum name="Integer" value="1" />
        <enum name="Numeric" value="2" />
        <enum name="Money" value="3" />
        <enum name="Date" value="4" />
        <enum name="DateTime" value="5" />
        <enum name="Guid" value="6" />
        <enum name="Boolean" value="7" />
        <enum name="Binary" value="8" />
	</attr>

<declare-styleable name="eruntech">
	<attr name="dataType"></attr>
	<attr name="isRequired" format="boolean"></attr>
	<attr name="collectSign" format="string"></attr>
	<attr name="empty2Null" format="boolean"></attr>
	<attr name="name" format="string"></attr>
	<attr name="collectName" format="string"></attr>
	<attr name="form" format="string"></attr>
	<attr name="sign" format="string"></attr>
	<attr name="sql" format="string"></attr>
	<attr name="isClosed" format="boolean"></attr>
	<attr name="autoLoad" format="boolean"></attr>
	<attr name="message" format="string"></attr>
	<attr name="confirmMessage" format="string"></attr>
	<attr name="stateHiddenId" format="string"></attr>
	<attr name="wantedStateValue" format="string"></attr>
	<attr name="funcSign" format="string"></attr>
	<!-- openForm废弃，以后使用open  -->
	<attr name="open" format="string"></attr>
	<attr name="orientation" format="integer"></attr>
	<attr name="displayValue" format="string"></attr>
	<attr name="bindValue" format="string"></attr>
	<attr name="layoutID" format="string"></attr>
	<attr name="bindID" format="string"></attr>
	<attr name="isEmpty" format="boolean"></attr>

	<attr name="textSize" format="dimension"></attr>
	<attr name="textColor" format="color"></attr>
	<attr name="clickedColor" format="color"></attr>

	<!--ESTextBox专用属性-->
	<!--必填项错误提示-->
	<attr name="isRequiredTooltip" format="string"></attr>
	<!--正则表达式-->
	<attr name="regularExpression" format="string"></attr>
	<!--正则表达式校验错误提示-->
	<attr name="regularTooltip" format="string"></attr>
</declare-styleable>
</resources>